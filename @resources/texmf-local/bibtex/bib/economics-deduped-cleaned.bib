@Article{wachter2010a,
  author   = {Wachter, Jessica A. and Yogo, Motohiro},
  journal  = {Review of Financial Studies},
  title    = {{Why do household portfolio shares rise in wealth?}},
  year     = {2010},
  issn     = {0893-9454},
