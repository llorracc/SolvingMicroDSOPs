@Article{Brueckner1997,
  author    = {Brueckner, Jan K.},
  journal   = {Journal of Real Estate Finance and Economics},
  title     = {{Consumption and Investment Motives and the Portfolio Choices of Homeowners}},
  year      = {1997},
  issn      = {0895-5638},
  number    = {2},
  pages     = {159--180},
  volume    = {15},
  abstract  = {This article investigates the portfolio choices of homeowners, taking into account the investment constraint introduced by Henderson and Ioannides (1983). This constraint requires housing investment by homeowners to be at least as large as housing consumption. It is shown that when the constraint is binding, the homeowner's optimal portfolio is ineffcient in a mean-variance sense. Thus, portfolio inefficiency is not an indication that consumers are irrational or careless in their financial decisions. Instead, inefficiency can be seen as the result of a rational balancing of the consumption benefits and portfolio distortion associated with housing investment.},
