@Article{rrUnbounded,
  author              = {Rinc{\'o}n-Zapatero, Juan Pablo and Rodr{\i}guez-Palmero, Carlos},
  journal             = {Econometrica},
  title               = {Existence and Uniqueness of Solutions to the Bellman Equation in the Unbounded Case},
