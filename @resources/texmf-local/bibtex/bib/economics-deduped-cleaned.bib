@Article{yao2005optimal,
  author    = {Yao, Rui and Zhang, Harold H.},
  journal   = {Review of Financial Studies},
  title     = {{Optimal consumption and portfolio choices with risky housing and borrowing constraints}},
  year      = {2005},
  issn      = {0893-9454},
  month     = {mar},
  number    = {1},
  pages     = {197--239},
  volume    = {18},
  abstract  = {We examine the optimal dynamic portfolio decisions for investors who acquire housing services from either renting or owning a house. Our results show that when indifferent between owning and renting, investors owning a house hold a lower equity proportion in their net worth (bonds, stocks, and home equity), reflecting the substitution effect, yet hold a higher equity proportion in their liquid portfolios (bonds and stocks), reflecting the diversification effect. Furthermore, following the suboptimal policy of always renting leads investors to overweigh in stocks, while following the suboptimal policy of always owning a house causes investors to underweigh in stocks.},
